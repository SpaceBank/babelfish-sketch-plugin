{
  "name": "babelfish",
  "description": "babelfish plugin",
  "version": "1.0.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SpaceBank/babelfish-sketch-plugin.git"
  },
  "skpm": {
    "name": "babelfish",
    "manifest": "src/manifest.json",
    "main": "babelfish.sketchplugin"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "render": "skpm-build --watch --run",
    "render:once": "skpm-build --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.4"
  },
  "dependencies": {
    "prop-types": "^15.5.8",
    "react": "^16.3.2",
    "react-sketchapp": "^3.0.0",
    "react-test-renderer": "^16.3.2"
  },
  "author": "BilousAlexandr <al.bilous@andersenlab.com>"
}
